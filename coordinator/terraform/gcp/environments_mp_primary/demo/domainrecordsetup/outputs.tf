output "domain_name" {
  description = "Domain that is managed with domain record setup"
  value       = local.domain_name
}

output "name_servers" {
  description = "Domain record nameservers associated with the zone generated by this module. To be added as NS records to the DNS provider which manages var.domain_name."
  value       = module.setupresources.name_servers
}
